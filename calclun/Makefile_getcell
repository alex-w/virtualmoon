
CFLAGS= -O3  -w -fPIC $(arch_flags) -L./cspice  -I./cspice
CC           = gcc
LIBS          = -lcspice -lm

SRC=getcell.c

OBJ=$(SRC:.c=.o)

obj_clean:
	rm -f $(OBJ)

cspice_all:
	$(MAKE) -j -C cspice all

cspice_clean:
	$(MAKE) -C cspice clean


clean: cspice_clean obj_clean

all: cspice_all $(OBJ)
